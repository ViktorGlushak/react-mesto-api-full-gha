{
    "env": {
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
        {
            "env": {
                "node": true
            },
            "files": [
                ".eslintrc.{js,cjs}"
            ],
            "parserOptions": {
                "sourceType": "script"
            }
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"]}],
      "no-param-reassign": 0,
      "consistent-return": 0,
      "max-len": ["error", {
        "code": 120
      }]
    }
}
